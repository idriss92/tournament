{
  "name": "tournament",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "bin": {
    "depcheck": "bin/depcheck"
  },
  "scripts": {
    "audit": "lighthouse http://localhost:8080",
    "depcheck": "node ./bin/depcheck",
    "depcheck-web": "node ./bin/depcheck --json | depcheck-web",
    "depcheck-json": "node ./bin/depcheck --json | babel-node ./build/check-json",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development webpack --debug --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "yarn run clean && cross-env NODE_ENV=production webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "clean": "rimraf build && rimraf compiled",
    "prestart": "babel-node tools/startMessage.js",
    "start": "yarn run build && cross-env NODE_ENV=production node compiled/server.js",
    "starte": "npm-run-all --parallel open:src lint:watch test:watch",
    "poststart": "audit",
    "open:src": "webpack-dev-server --progress --colors",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "yarn run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.tsx\"",
    "test:watch": "yarn run test",
    "test-dependent": "node-version-gte-4 && dependent-build || node-version-lt-4",
    "test-coverage": "babel-node node_modules/cross-env/bin/cross-env.js NODE_ENV=test nyc mocha ./test ./test/special --timeout 20000 && nyc report --reporter=text-lcov > ./coverage/coverage.lcov",
    "watch": "webpack -d --watch"
  },
  "author": {
    "name": "Idriss SANI",
    "email": "idriss_sani@yahoo.fr"
  },
  "license": "ISC",
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.2",
